# База данных
MONGODB_URI=mongodb://localhost:27017/webreglog
DB_NAME=webreglog

# JWT
JWT_SECRET=webreglog-jwt-secret-key-2024-production-change-this
JWT_EXPIRES_IN=7d

# Сервер
PORT=5000
NODE_ENV=production

# CORS
FRONTEND_URL=http://localhost:3000

# Настройки загрузки файлов
MAX_FILE_SIZE=5242880
UPLOAD_PATH=uploads/

# Настройки безопасности
BCRYPT_ROUNDS=12
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100
